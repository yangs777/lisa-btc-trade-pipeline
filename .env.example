# Binance API Configuration
BINANCE_API_KEY=your_binance_api_key_here
# Note: Secret key will be entered at runtime for security

# Google Cloud Configuration
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json
GCP_PROJECT_ID=my-project-779482
GCS_BUCKET_NAME=btc-orderbook-data

# Monitoring Configuration
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_here
PROMETHEUS_REMOTE_WRITE_URL=http://localhost:9090/api/v1/write

# Trading Configuration (can override config.yaml)
TRADING_MODE=paper  # paper or live
MAX_LEVERAGE=2      # Override for testing
LOG_LEVEL=INFO      # DEBUG, INFO, WARNING, ERROR

# Model Configuration
MODEL_CACHE_DIR=./models
CHECKPOINT_DIR=./checkpoints

# API Server Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4